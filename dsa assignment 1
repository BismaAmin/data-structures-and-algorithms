// with ommiting the namespace
#include<iostream>
using namespace std;

int main(){
    int num [6];
    
    for(int i=0; i<6; i++){
       cout<<"Enter a number "<< i+1 << ":" <<endl;
       cin>>num[i];
    }
    
    for (int i=0; i<6; i++){
       cout<<"The number you entered is = "<< num[i] <<endl;
    }
    
}


//without ommiting the namespace 
#include<iostream>

int main(){
    int num [6];
    
    for(int i =0; i<6; i++){
       std::cout<<"Enter a number "<< i+1 << ":" <<std::endl;
       std::cin>>num[i];
    }
    
    for (int i=0; i<6; i++){
       std::cout<<"The number you entered is = "<< num[i] << std::endl;
    }
    
}
